{
	"name": "beewriteversion",
	"displayName": "beewriteversion",
	"description": "补充上html版本号",
	"publisher": "ameol",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.writeVersion"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.writeVersion",
				"title": "Hello World"
			}
		],
		"keybindings": {
			"command": "extension.writeVersion",
			"win": "ctrl+s",
			"mac": "cmd+s",
			"when": "editorTextFocus"
		},
		"configuration": {
			"title": "write-version",
			"properties": {
				"writeVersion.includes": {
					"type": "array",
					"default": [],
					"description": "所包含的要自动补上版本号的文件目录名"
				}
			}
		}
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
